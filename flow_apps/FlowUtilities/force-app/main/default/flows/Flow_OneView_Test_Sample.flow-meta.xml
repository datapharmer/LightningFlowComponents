    <formulas>
        <name>fCaseSafeId</name>
        <dataType>String</dataType>
        <expression>CASESAFEID( LEFT({!vTestId},15) )</expression>
    </formulas>
    <formulas>
        <name>fDeployVersion</name>
        <dataType>String</dataType>
        <expression>{!vFlowAPIName} &amp; &quot;-&quot; &amp; TEXT( {!vDeployVersion} )</expression>
    </formulas>
    <formulas>
        <name>fEscapeMetadata</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE( SUBSTITUTE( {!vSourceMetadata}, &quot;{&quot;, &quot;&amp;lbrace;&quot; ), &quot;}&quot;, &quot;&amp;rbrace;&quot; )</expression>
    </formulas>
    <formulas>
        <name>fFindReplacedBrace</name>
        <dataType>Boolean</dataType>
        <expression>OR( FIND( &quot;\\{&quot;, {!vDeployMetadata} )&gt;0, FIND( &quot;\\}&quot;, {!vDeployMetadata} )&gt;0 )</expression>
    </formulas>
    <formulas>
        <name>fFullFlowName</name>
        <dataType>String</dataType>
        <expression>{!vFlowName} &amp; IF( {!vDeployVersion} &gt; 0, &quot; - &quot; &amp; TEXT( {!vDeployVersion} ), &quot;&quot; )</expression>
    </formulas>
    <formulas>
        <name>fHardcodedId</name>
        <dataType>String</dataType>
        <expression>&quot;[&quot; &amp; &quot;0698c00000Ec4CDAAZ&quot; &amp; &quot;]&quot;</expression>
    </formulas>
    <formulas>
        <name>fHighlightFormat</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE( SUBSTITUTE( SUBSTITUTE( SUBSTITUTE( SUBSTITUTE( BLANKVALUE( {!vOverrideFormat}, {!ttHighlightFormat_01} ), &quot;&amp;&quot;, &quot;&amp;amp;&quot; ), &quot;&lt;&quot;, &quot;&amp;lt;&quot; ), &quot;&gt;&quot;, &quot;&amp;gt;&quot; ), &quot;\&quot;&quot;, &quot;&amp;quot;&quot; ), &quot;&apos;&quot;, &quot;&amp;apos;&quot; )</expression>
    </formulas>
    <formulas>
        <description>Replace { } with \{ \} so they don&apos;t get dropped in the clipboard.  This will require a manual re-replacement once imported back into VSCode.</description>
        <name>fReplaceBraces</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE( SUBSTITUTE( {!vDeployMetadata}, &quot;&amp;lbrace;&quot;, &quot;\\{&quot; ), &quot;&amp;rbrace;&quot;, &quot;\\}&quot; )</expression>
    </formulas>
    <formulas>
        <name>fSourceCharacters</name>
        <dataType>Number</dataType>
        <expression>LEN( {!vSourceMetadata} )</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>fSourceVersion</name>
        <dataType>String</dataType>
        <expression>{!vFlowAPIName} &amp; &quot;-&quot; &amp; TEXT( {!vSourceVersion} )</expression>
    </formulas>
    <formulas>
        <name>fUnescapeMetadata</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE( SUBSTITUTE( {!vDeployMetadata}, &quot;&amp;lbrace;&quot;, &quot;{&quot; ), &quot;&amp;rbrace;&quot;, &quot;}&quot; )</expression>
    </formulas>
    <formulas>
        <name>fUserId</name>
        <dataType>String</dataType>
        <expression>{!$User.Id}</expression>
    </formulas>
